<div>
    @Html.ActionLink("OLX", "Olx", "Parse")|
    @Html.ActionLink("Pr5", "Weather", "Parse")
</div>
<h2>Olx</h2>

@using (Html.BeginForm("ParseOlx", "Parse", FormMethod.Post, new { id = "parseForm" }))
{
    <input type="text" name="product" class="form-control" placeholder="Предмет" />
    <input type="text" name="city" class="form-control" placeholder="Город" />
    <input type="submit" name="ok" class="btn-success" />
}
<hr>
<div id="result"></div>
@section scripts
{
    <script type="text/javascript">

        $(function () {

            $("#parseForm").on("submit", function (e) {
                e.preventDefault();

                $.ajax({
                    url: this.action,
                    type: this.method,
                    data: $(this).serialize(),
                    success: function (data) {
                        $("#result").html(data);
                    }
                });
            });
        });
    </script>
}